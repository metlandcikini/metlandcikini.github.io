{
  "locals": {
    "url": "https://metlandcikini.github.io",
    "logoUrl": "https://metlandcikini.github.io/cikini.jpg",
    "name": "CLUSTER METLAND CIKINI",
    "owner": "GROUP METLAND CIKINI",
    "description": "Perumahan metland cluster taman cikini",
    "sitemap": "sitemap.xml",
    "discuss_id": "metlandcikini"
  },
  "plugins": [
    "./plugins/paginator.coffee",
    "wintersmith-sassify",
    "wintersmith-contents",
    "wintersmith-robots",
    "wintersmith-sitemap",
    "wintersmith-tag"
  ],
  "require": {
    "moment": "moment",
    "_": "underscore",
    "typogr": "typogr"
  },
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 10
  },
  "sassify": {
    "outputStyle": "compressed",
    "sourceComments": false,
    "sourceMap": false
  },
  "tag": {
      "perPage": 10
  },
  "jsbundler": {
    "sourceMap": false,
    "minify": true
  }
}
